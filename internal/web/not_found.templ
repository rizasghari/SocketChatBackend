package web

templ NotFound() {
	<div class="d-flex flex-column align-items-center justify-content-center" style="height: 100vh; background-color: #f8f9fa;">
		<h1 class="display-1">404</h1>
		<h2 class="mb-4">Page Not Found</h2>
		<p class="mb-4">The page you are looking for might have been removed, had its name changed, or is temporarily unavailable.</p>
		<a href="/" class="btn btn-primary">Go to Homepage</a>
	</div>
}
